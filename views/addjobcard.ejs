<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Add Job Card</title>
    <link rel="stylesheet" href="/CSS/styles.css" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" />

    <!-- jQuery library -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
  </head>
  <body>
    <nav class="navbar job">
      <span class="navbar-brand mb-0 head">ADD JOBCARD</span>
    </nav>
    <form action="/addJobCard" method="post">
      <div class="jobcard">
        <container class="jobcard">
          <form>
            <div class="form-group">
              <label for="exampleFormControlInput1">Vehicle No</label>
              <input
                type="text"
                name="carNumber"
                class="form-control"
                id="exampleFormControlInput1"
                placeholder="MH10AA3026" />
            </div>
            <div class="form-group">
              <label for="exampleFormControlSelect1">Car Model</label>
              <input
                list="browsers"
                name="car"
                id="browser"
                class="form-control" />
              <datalist id="browsers">
                <option value="Maruti Suzuki Swift"></option>
                <option value="Maruti Suzuki Swift Dzire"></option>
                <option value="Maruti Suzuki Ignis"></option>
                <option value="Maruti Suzuki Ciaz"></option>
                <option value="Maruti Suzuki Ertiga"></option>
              </datalist>
            </div>
            <div class="form-group">
              <label for="exampleFormControlInput1">Customer Name</label>
              <input
                type="text"
                name="customerName"
                class="form-control"
                id="exampleFormControlInput1"
                placeholder="Dipak Mane" />
            </div>
            <div class="form-group">
              <label for="exampleFormControlInput1">Contact No</label>
              <input
                name="contactNumber"
                type="number"
                class="form-control"
                id="exampleFormControlInput1"
                placeholder="9011368197" />
            </div>
            <div class="col-md-12 text-center">
              <button type="submit" class="btn btn-secondary btn-lg">
                Add Jobcard
              </button>
            </div>
          </form>
        </container>
      </div>
    </form>
  </body>
</html>
